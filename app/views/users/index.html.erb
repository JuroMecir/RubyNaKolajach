<% provide(:title, 'All users') %>
<h1>All users</h1>


<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to  'show', user, class: "btn btn-primary" %>
      <%= user.name %>
      <%= user.surname %>
      <%= user.email %>

    </li>


  <% end %>

</ul>

<%= will_paginate %>
